<?xml version="1.0" encoding="UTF-8"?>
<root>
  <model>
    <name>User</name>
    <attribute>
      <title>Name</title>
      <validators>
        <presence>true</presence>
        <datatype>string</datatype>
        <maxlength>10</maxlength>
        <minlength>2</minlength>
        <wrong>validator</wrong><!-- an additonal validator that do not comply with the XSD Schema for the document, to provoque error -->
      </validators>
    </attribute>
    <attribute>
      <title>familyname</title>
      <validators>
        <presence>false</presence>
        <datatype>string</datatype>
      </validators>
    </attribute>
    <attribute>
      <title>birthdate</title>
      <validators>
        <presence>true</presence>
        <datatype>date</datatype>
      </validators>
    </attribute>
    <attribute>
      <title>time</title>
      <validators>
        <presence>true</presence>
        <datatype>datetime</datatype>
      </validators>
    </attribute>
    <attribute>
      <title>email</title>
      <validators>
        <presence>true</presence>
        <datatype>email</datatype>
      </validators>
    </attribute>
    <attribute>
      <title>zip_code</title>
      <validators>
        <presence>true</presence>
        <datatype>integer</datatype>
      </validators>
    </attribute>
    <attribute>
      <title>ratio</title>
      <validators>
        <presence>true</presence>
        <datatype>float</datatype>
      </validators>
    </attribute>
    <attribute>
      <title>telephone</title>
      <validators>
        <presence>true</presence>
        <datatype>telephone</datatype>
      </validators>
    </attribute>
    <attribute>
      <title>website</title>
      <validators>
        <presence>true</presence>
        <datatype>url</datatype>
      </validators>
    </attribute>
    <attribute>
      <title>distribution_share</title>
      <validators>
        <presence>true</presence>
        <datatype>percentage</datatype>
      </validators>
    </attribute>
  </model>
</root>
